@model IEnumerable<Dist23Bridge.Jail>

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JailName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JailAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JailContact)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JailContPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.JailContEmail)
            </th>
            <th></th>
        </tr>
    
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JailName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JailAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JailContact)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JailContPhone)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JailContEmail)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.jail_id }) |
                @Html.ActionLink("Details", "Details", new { id=item.jail_id }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.jail_id })
            </td>
        </tr>
    }
    
    </table>
</body>
</html>
